\def\autheraff#1{\gdef\@autheraff{#1}}
\def\reportauther#1{\gdef\@reportauther{#1}}
\renewcommand{\maketitle}{%
  \begin{titlepage}%
    \let\footnotesize\small
    \let\footnoterule\relax
    \let\footnote\thanks
%    \null\vfil
%    \vskip 2.5\p@
    \begin{center}%
{\Large
	\vspace*{-2em}
	\@reportname
\par}
	\vskip 14em
	{\bf {\LARGE \@reporttitle \par} }
	\vskip 7em
	{\large \@autheraff \par}
	\vskip 5em
	{\LARGE \@reportauther \par}
	\vskip 9em
	{\large \@reportdate}
	\vspace*{-2em}
    \@thanks\vfil\null
	\end{center}
  \end{titlepage}%
